<h1>Onboarding</h1>

<h3>Basic information</h3>

<.form let={f} for={@changeset} action={Routes.onboarding_path(@conn, :update)} phx-change="validate" phx-submit="save"%>
  

  
  <%= label f, :username %>
  <%= text_input f, :username %>
  <%= error_tag f, :username %>
  
  <%= label f, :gender %>
  <%= text_input f, :gender%>
  <%= error_tag f, :gender %>

  <div>
    <%= submit "SAVE CHANGES" %>
  </div>
</.form>
